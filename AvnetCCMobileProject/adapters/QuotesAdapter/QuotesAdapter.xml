<wl:adapter name="QuotesAdapter" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:wl="http://www.ibm.com/mfp/integration" xmlns:http="http://www.ibm.com/mfp/integration/http">

	<displayName>QuotesAdapter</displayName>
	<description>QuotesAdapter</description>
	<connectivity>
		<connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
			<protocol>https</protocol>
			<domain>r2om-test.avnet.com</domain>
			<port>443</port>
			<connectionTimeoutInMilliseconds>30000</connectionTimeoutInMilliseconds>
			<socketTimeoutInMilliseconds>30000</socketTimeoutInMilliseconds>
			<maxConcurrentConnectionsPerNode>50</maxConcurrentConnectionsPerNode>
			<!-- Following properties used by adapter's key manager for choosing specific 
				certificate from key store <sslCertificateAlias></sslCertificateAlias> <sslCertificatePassword></sslCertificatePassword> -->
		</connectionPolicy>

	</connectivity>
	<procedure name="getQuotesList" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest" />
	<procedure name="getSessionIdAndCookie" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest"/>
	<procedure name="getQuotesDetails" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest"/>
	<procedure name="getLineItems" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest"/>
	<procedure name="editRequestName" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest"/>
	<procedure name="addNote" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest"/>
	<procedure name="editEndUser" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest"/>
	
</wl:adapter>
