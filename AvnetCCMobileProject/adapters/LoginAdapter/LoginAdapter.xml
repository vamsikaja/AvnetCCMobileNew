<?xml version="1.0" encoding="UTF-8"?>
<wl:adapter name="LoginAdapter"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:wl="http://www.ibm.com/mfp/integration"
	xmlns:http="http://www.ibm.com/mfp/integration/http">

	<displayName>LoginAdapter</displayName>
	<description>Adapter for Authentication</description>
	<connectivity>
		<connectionPolicy xsi:type="http:HTTPConnectionPolicyType">
			<protocol>https</protocol>
			<domain>r2om-test.avnet.com</domain>
			<port>443</port>	
			<connectionTimeoutInMilliseconds>30000</connectionTimeoutInMilliseconds>
			<socketTimeoutInMilliseconds>30000</socketTimeoutInMilliseconds>
			<maxConcurrentConnectionsPerNode>50</maxConcurrentConnectionsPerNode>
		</connectionPolicy>
	</connectivity>
	<procedure name="login" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest" />
	<procedure name="logout" connectAs="endUser" securityTest="ISAMforWorklight-mobilesecurityTest" />
</wl:adapter>
